## Use c++11
CXX_STD = CXX11

CC=/usr/local/bin/gcc-4.9
CXX=/usr/local/bin/g++-4.9

PKG_CXXFLAGS = -I. -Isrc -Ithird_party -Ibayesopt -DR_BUILD
PKG_LIBS = "/usr/local/lib/libbayesopt.a" "/usr/local/lib/libnlopt.a"

SOURCES = $(wildcard *.cpp */*.cpp */*/*.cpp */*/*/*.cpp)
OBJECTS = $(SOURCES:.cpp=.o)
